              "script": {
                "exec": [
                  "pm.test(\"Retorno deve conter 100 posts\", function () {",
                  "    pm.response.to.have.status(200);",
                  "    const dados = pm.response.json();",
                  "    pm.expect(dados).to.be.an(\"array\").with.lengthOf(100);",
                  "});",
                  "",
                  "pm.test(\"Cada post deve ter 'userId', 'id', 'title' e 'body'\", function () {",
                  "    const dados = pm.response.json();",
                  "    dados.forEach(post => {",
                  "        pm.expect(post).to.have.property(\"userId\");",
                  "        pm.expect(post).to.have.property(\"id\");",
                  "        pm.expect(post).to.have.property(\"title\");",
                  "        pm.expect(post).to.have.property(\"body\");",
